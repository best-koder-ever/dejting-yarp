{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "userRoute": {
        "ClusterId": "userCluster",
        "Match": {
          "Path": "/api/userprofiles/{**catch-all}"
        }
      },
      "swipeRoute": {
        "ClusterId": "swipeCluster",
        "Match": {
          "Path": "/api/swipes/{**catch-all}"
        }
      },
      "matchmakingRoute": {
        "ClusterId": "matchmakingCluster",
        "Match": {
          "Path": "/api/matchmaking/{**catch-all}"
        }
      },
      "photoRoute": {
        "ClusterId": "photoCluster",
        "Match": {
          "Path": "/api/photos/{**catch-all}"
        }
      },
      "messagingRoute": {
        "ClusterId": "messagingCluster",
        "Match": {
          "Path": "/api/messages/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "authService": {
            "Address": "http://localhost:8081/"
          }
        }
      },
      "userCluster": {
        "Destinations": {
          "userService": {
            "Address": "http://localhost:8082/"
          }
        }
      },
      "swipeCluster": {
        "Destinations": {
          "swipeService": {
            "Address": "http://localhost:8087/"
          }
        }
      },
      "matchmakingCluster": {
        "Destinations": {
          "matchmakingService": {
            "Address": "http://localhost:8083/"
          }
        }
      },
      "photoCluster": {
        "Destinations": {
          "photoService": {
            "Address": "http://localhost:8085/"
          }
        }
      },
      "messagingCluster": {
        "Destinations": {
          "messagingService": {
            "Address": "http://localhost:8086/"
          }
        }
      }
    }
  }
}
